{
  "name": "react-app-environment-variables",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "sh -ac '. .env.dev; react-scripts start",
    "build": "sh -ac '. .env.dev; react-scripts build",
    "test": "sh -ac '. .env.dev; react-scripts test",
    "start-prod": "sh -ac '. .env.production; react-scripts start'",
    "build-prod": "sh -ac '. .env.production; react-scripts build",
    "test-prod": "sh -ac '. .env.production; react-scripts test",
    "start-test": "sh -ac '. .env.test; react-scripts start'",
    "build-test": "sh -ac '. .env.test; react-scripts build",
    "test-test": "sh -ac '. .env.test; react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
